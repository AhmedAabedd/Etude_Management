<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data noupdate="1">
       <record id="cron_update_session_stored_status" model="ir.cron">
           <field name="name">Update Session Status</field>
           <field name="model_id" ref="model_etude_session"/>
           <field name="state">code</field>
           <field name="code">model._cron_update_session_status()</field>
           <field name="user_id" ref="base.user_root"/>
           <field name='interval_number'>15</field>
           <field name='interval_type'>minutes</field>
           <field name="nextcall"
                  eval="(datetime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
           <field name="priority">1</field>
       </record>
   </data>
</odoo>
